--Made by Letziu
--Every Second You Get +1 Jump Power Script

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("AutoHub", "Ocean")

--TABS
local Farm = Window:NewTab("Farm")
local Player = Window:NewTab("Player")
local Teleports = Window:NewTab("Teleports")
local Credits = Window:NewTab("Made by Letziu")

--SECTIONS 
local NormalTower = Farm:NewSection("Normal Tower")
local RedTower = Farm:NewSection("Red Tower")
local LavaTower = Farm:NewSection("Lava Tower")
local IceTower = Farm:NewSection("Ice Tower")
local FlowerTower = Farm:NewSection("Flower Tower")
local SnowTower = Farm:NewSection("Snow Tower")
local DarkTower = Farm:NewSection("Dark Tower")
local VoidTower = Farm:NewSection("Void Tower")
local DesertTower = Farm:NewSection("Desert Tower")
local ForestTower = Farm:NewSection("Forest Tower")
local CharacterMods = Player:NewSection("Character Mods")
local Owner = Credits:NewSection("This script has been brought to you by Letziu!")
local Teleports = Teleports:NewSection("Teleports")
--TOGGLES
NormalTower:NewToggle("Normal Tower", "This tower NEEDS no win!", function(state)
    if state then
		_G.TP1 = true;

while _G.TP1 do
wait()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1.76928818, 1881.95239, 128.153885, -0.999995053, 0.000263258873, 0.00313021312, 7.98554911e-09, 0.996482253, -0.0838041306, -0.0031412642, -0.0838037208, -0.996477306)
wait(8)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1.76928818, 1881.95239, 128.153885, -0.999995053, 0.000263258873, 0.00313021312, 7.98554911e-09, 0.996482253, -0.0838041306, -0.0031412642, -0.0838037208, -0.996477306)
wait(3)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1.76928818, 1881.95239, 128.153885, -0.999995053, 0.000263258873, 0.00313021312, 7.98554911e-09, 0.996482253, -0.0838041306, -0.0031412642, -0.0838037208, -0.996477306)
end
		print("Toggled Off NormalTower")
    else
        _G.TP1 = false
		print("Stopping the last cycle...")
		wait(8)
		print("Toggle NormalTower ready for start")
    end
end)

