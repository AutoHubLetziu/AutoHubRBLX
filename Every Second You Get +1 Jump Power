Teleports:NewButton("Lobby", "Teleports you to the lobby", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(6.60959435, 0.449998498, -122.411415, -1, 0, 0, 0, 1, 0, 0, 0, -1)
	print("TPed back to lobby")
end)

